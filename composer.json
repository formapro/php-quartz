{
    "name": "formapro/php-quartz",
    "type": "library",
    "description": "Job Time Scheduler Library",
    "keywords": ["job", "time", "task", "time scheduler", "quartz", "chrono"],
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "ramsey/uuid": "^2|^3.5",
        "enqueue/enqueue": "^0.3|^0.4",
        "makasim/yadm": "*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5",
        "symfony/console": "^2.8|^3",
        "symfony/config": "^2.8|^3"
    },
    "autoload": {
        "psr-4": { "Quartz\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
