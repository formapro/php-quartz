{
    "name": "php-quartz/quartz-dev",
    "type": "project",
    "minimum-stability": "dev",
    "description": "PHP Job Time Scheduler Project",
    "keywords": ["job", "time", "task", "time scheduler", "quartz", "chrono"],
    "license": "MIT",

    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "php": ">=5.6",
        "formapro/php-quartz": "*@dev",
        "formapro/php-quartz-bridge": "*@dev",
        "formapro/php-quartz-app": "*@dev",
        "symfony/dotenv": "^3.3",
        "phpunit/phpunit": "~5.5"
    },
    "repositories": [
        {
            "type": "path",
            "url": "pkg/quartz"
        },
        {
            "type": "path",
            "url": "pkg/bridge"
        },
        {
            "type": "path",
            "url": "pkg/app"
        }
    ]
}
